@model CreateCargoCompanyDto
@{
    ViewData["Title"] = "CreateCargoCompany";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<section class="content container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="chart-box">
                <h4>Kargo Şirketi Giriş Sayfası</h4>
                <div class="bs-example" data-example-id="bordered-table">
                    <form method="post">
                        <label for="CompanyName">Kargo Şirketi Adı</label>
                        <input type="text" class="form-control" asp-for="CargoCompanyName" />
                        <br />
                        <button class="btn btn-primary">Kargo Şirketi Kaydet</button>
                    </form>

                </div>
            </div>
        </div>
    </div>
</section>