@model GetCargoCustomerByIdDto
@{
    ViewData["Title"] = "UserAddressInfo";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}


<section class="content container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="chart-box">
                <div class="bs-example" data-example-id="bordered-table">
                    <br />
                    <br />
                    <h3>User Details</h3>
                    <div class="alert alert-info">Kullanıcı Kargo Id: @Model.Id</div>
                    <div class="alert alert-warning">İsim:        @Model.Name @Model.Surname</div>
                    <div class="alert alert-info">Email:       @Model.Email</div>
                    <div class="alert alert-warning">Telefon:     @Model.PhoneNumber</div>
                    <div class="alert alert-info">İlçe/Şehir:  @Model.District/@Model.City</div>
                    <div class="alert alert-warning">Adres:       @Model.Address</div>
                </div>
            </div>
        </div>
    </div>
</section>





